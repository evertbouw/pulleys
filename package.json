{
  "name": "@evertbouw/pulleys",
  "version": "0.2.0",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "./lib"
  ],
  "sideEffects": false,
  "repository": "git@github.com:evertbouw/pulleys.git",
  "homepage": "https://evertbouw.github.io/pulleys",
  "author": "Evert Bouw <bouw@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "rimraf ./lib && pnpm build:esm && pnpm build:cjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "fmt": "prettier --write src/*.{ts,tsx}",
    "docs:generate": "typedoc src/index.ts",
    "docs:deploy": "gh-pages -d docs"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "react": ">16.8.0",
    "react-dom": ">16.8.0"
  },
  "optionalPeerDependencies": {
    "@types/react": ">16.8.0",
    "@types/react-dom": ">16.8.0"
  }
}
